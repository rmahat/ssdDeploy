<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE Custom PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Custom name="File Attachment Parameters">
  <Attributes>
    <Map>
      <entry key="basePath" value="//MyServer/storagebasepath"/>
      <entry key="baseUrl" value="http://myserver/attachments"/>
      <entry key="maxLimit" value="2"/>
      <entry key="maxSizeInMB" value="2"/>
      <entry key="permittedExtensions" value="doc,pdf,xls,csv,ppt,pptx,xlsx,docx,jpg,png,gif,txt"/>
    </Map>
  </Attributes>
</Custom>